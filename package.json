{
  "name": "FalseBott",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch source/main.ts | winston-pretty",
    "lint": "eslint --fix . && prettier --write .",
    "setup": "./scripts/setup.sh",
    "start": "node build/main.js",
    "test": "vitest"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.35",
    "awilix": "^10.0.2",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "env-schema": "^6.0.0",
    "openai": "^4.54.0",
    "utf-8-validate": "^6.0.4",
    "winston": "^3.13.1",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^22.1.0",
    "eslint": "^8.57.0",
    "eslint-config-unjs": "^0.3.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "winston-pretty": "^1.1.1"
  },
  "lint-staged": {
    "*.{mjs,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
